<a href="#/new" class="btn btn-primary">New User</a>
<hr />

<div>
<h4>Search: </h4><input type="text" id="content" value="<%= search ? search : "" %>"/>
</div>
<hr />
<table class ="table striped">
    <thead>
		<tr>
			<th>Name</th>
			<th>Email</th>
			<th>Age</th>
			<th></th>
		</tr>
	</thead>
    <tbody>
	 <% _.each(users, function(user){ %>
	    <tr>
		    <td><%= user.get("name") %></td>
		    <td><%= user.get("email") %></td>
		    <td><%= user.get("age") %></td>
		    <td><a href="#/edit/<%= user.id %>" class="btn">Edit</a></td>
	    </tr>
	  <% }); %>
    </tbody>
</table>